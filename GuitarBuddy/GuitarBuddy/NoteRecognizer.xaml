<Window x:Class="GuitarBuddy.NoteRecognizer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GuitarBuddy"
        mc:Ignorable="d"
        Title="NoteRecognizer" Height="450" Width="800">
    <Grid>
        <TextBlock FontSize="80" Text="String:"/>
        <TextBlock Margin="230 0 0 0" FontSize="80" Text="{Binding Fret}"/>
    </Grid>
</Window>
